{
    "name": "@slimio/unzipper",
    "version": "0.2.0",
    "description": "SlimIO unzipper",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && nyc --reporter=lcov node test/test.js",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "codecov",
        "report": "nyc report --reporter=html",
        "benchmark": "node benchmark/index"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/unzipper.git"
    },
    "keywords": [
        "yauzl",
        "unzipper",
        "zip",
        "unzip",
        "extract"
    ],
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/unzipper/issues"
    },
    "homepage": "https://github.com/SlimIO/unzipper#readme",
    "dependencies": {
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^13.1.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/is": "^1.5.1",
        "@slimio/nodejs-downloader": "^0.4.0",
        "@slimio/psp": "^0.11.1",
        "@types/yauzl": "^2.9.1",
        "codecov": "^3.7.0",
        "cross-env": "^7.0.2",
        "del": "^5.1.0",
        "eslint": "^7.0.0",
        "extract-zip": "^2.0.0",
        "husky": "^4.2.5",
        "japa": "^3.0.1",
        "jsdoc": "^3.6.4",
        "nyc": "^15.0.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "npm": ">=6.0.0",
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
