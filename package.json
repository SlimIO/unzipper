{
    "name": "@slimio/unzipper",
    "version": "0.0.1",
    "description": "SlimIO unzipper",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc ava",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/unzipper.git"
    },
    "keywords": [
        "SlimIO",
        "monitoring",
        "yauzl",
        "unzipper"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/unzipper/issues"
    },
    "homepage": "https://github.com/SlimIO/unzipper#readme",
    "dependencies": {
        "@slimio/is": "^1.4.0",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.3",
        "@types/yauzl": "^2.9.1",
        "ava": "^1.2.1",
        "cross-env": "^5.2.0",
        "del": "^3.0.0",
        "eslint": "^5.13.0",
        "husky": "^1.3.1",
        "japa": "^2.0.7",
        "jsdoc": "^3.5.5",
        "nyc": "^13.2.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "npm": ">=6.0.0",
        "node": ">=10.1.0"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env npm test && eslint index.js",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
